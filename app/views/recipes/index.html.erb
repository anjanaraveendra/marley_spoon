<div class="row">
  <% @recipes.each do |recipe| %>
    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
      <a href="<%= recipe_path(recipe) %>">
        <img src="<%= request.protocol + recipe.image_url %>" class="img-fluid" alt="">
        <%= recipe.title %>
      </a>
    </div>
  <% end %>
</div>